<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>ImageManagementUtility</display-name>
	 
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.LoginServlet</servlet-class>
	</servlet>
<servlet>
		<servlet-name>ImageEditServlet</servlet-name>
		<display-name>ImageEditServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.ImageEditServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImageGeneratorServlet</servlet-name>
		<display-name>ImageGeneratorServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.ImageGeneratorServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImageDeleteServlet</servlet-name>
		<display-name>ImageDeleteServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.ImageDeleteServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PasswordResetServlet</servlet-name>
		<display-name>PasswordResetServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.PasswordResetServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ImageServlet</servlet-name>
		<display-name>ImageServlet</display-name>
		<description></description>
		<servlet-class>com.nagarro.controller.ImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageEditServlet</servlet-name>
		<url-pattern>/ImageEditServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageGeneratorServlet</servlet-name>
		<url-pattern>/ImageGeneratorServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageDeleteServlet</servlet-name>
		<url-pattern>/ImageDeleteServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PasswordResetServlet</servlet-name>
		<url-pattern>/PasswordResetServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/ImageServlet</url-pattern>
	</servlet-mapping>
</web-app>
